{
  "extends": "../../tsconfig.common.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["ES5", "DOM", "ES2015.promise"],
    "module": "es6",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "noImplicitReturns": true,
    "sourceMap": true,
    "skipLibCheck": false,
    "skipDefaultLibCheck": true
  },
  "exclude": ["node_modules"]
}
