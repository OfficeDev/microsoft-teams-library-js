{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "noImplicitReturns": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "target": "es5"
  },
  "exclude": ["node_modules"]
}
